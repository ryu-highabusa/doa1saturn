	<!--- イベント --->
	<events>
<!-- 更新イベント -->
		<!--E_Key外枠全体-->
		<event:composite id="update_dlg_input_ecard">
		</event:composite>

		<!--フォーム 更新イベント-->
		<event:composite id="update_form_ecard">
			<!-- 被覆状態のチェック -->
			<or>
				<condition>
					<and>
						<!--押下状態じゃなかったら-->
						<not><predicate_point_button_on button="left"/></not>
						<predicate_point_hover_in/>
					</and>
					<event:composite>
						<or>
							<!--押下状態じゃなかったら被覆のSEを鳴らす-->
							<condition><not><predicate_hold/></not>
								<play_sound:eden label="se_m_over"/>
							</condition>
						</or>
						
						<change_cursor label="button"/>
						<hover_enable/>
					</event:composite>
				</condition>
				
				<condition><predicate_point_hover_out/>
					<event:composite>
						<hover_disable/>
						<restore_cursor/>
					</event:composite>
				</condition>
			</or>
			
			<!-- クリックのチェック -->
			<or>
				<!--他をクリックしたらアクティブ状態解除-->
				<condition>
					<and>
						<not><predicate_point_collide/></not>
						<predicate_point_button_on button="left"/>
						<predicate_active/>
					</and>
					<active_disable/>
				</condition>
				
				<!--左クリック-->
				<condition><predicate_point_hold_in button="left"/>
					<event:composite>
						<active_enable/>
						<hook id="hold_in_form_ecard"/>
						<hold_enable/>
					</event:composite>
				</condition>
				<!-- 被覆しておらずボタンが離されたら、メニュー要素の押下状態を無効にする。-->
				<condition><not><predicate_point_hold_out button="left"/></not>
					<condition><predicate_point_button_off button="left"/>
						<hold_disable/>
					</condition>
				</condition>
				<condition><predicate_point_hold_out button="left"/>
					<event:composite>
						<hook id="hold_out_form_ecard"/>
						<hold_disable/>
					</event:composite>
				</condition>
			</or>

		</event:composite>

		<!--フォームimm部分 更新イベント-->
		<event:composite id="update_sfnt_ek_ip">
			<or>
				<!--フォームがアクティブなら、入力モード-->
				<condition><other component="form_ecard"><predicate_active/></other>
					<event:composite>
						<imm_enable/>
					</event:composite>
				</condition>
				<!--それ以外は、非入力モード-->
				<event:composite>
					<imm_disable/>
				</event:composite>
			</or>
		</event:composite>
		
