	<!--- イベント --->
	<events>
	
<!-- 更新イベント -->
		<event:composite id="update_dlg_win">
			<!-- 被覆状態のチェック -->
			<or>
				<condition>
						<predicate_point_hover_in/>
					<event:composite>
						<hover_enable/>
						<change_cursor label="mouse_over"/>
					</event:composite>
				</condition>
				<condition>
						<predicate_point_hover_out/>
					<event:composite>
						<hover_disable/>
						<restore_cursor/>
					</event:composite>
				</condition>
			</or>
		
			<or>
				<!-- 左クリック -->
				<condition><predicate_point_hold_in button="left"/>
					<event:composite>
						<drag drag="ready"/>
						<hold_enable/>
					</event:composite>
				</condition>

				<!-- ドラッグ開始 -->
				<condition>
					<and>
						<predicate_drag_start button="left" threshold="0.001"/>
						<predicate_hold/>
					</and>
					<event:composite>
						<push_drag/>
						<drag drag="start"/>
						<hook id="drag_start_option_main"/>
					</event:composite>
				</condition>

				<!-- ドラッグ終了 -->
				<condition>
					<and>
						<predicate_drag drag="start"/>
						<predicate_point_button_off button="left"/>
					</and>
					<event:composite>
						<clear_drags/>
						<drag drag="ready"/>
						<hold_disable/>
						<hook id="drag_end_dlg_option_main"/>
					</event:composite>
				</condition>

				<!-- ドラッグ中は移動 -->
				<condition>
					<predicate_drag drag="start"/>
					<iterate_drags>
						<center_drag button="left"/>
					</iterate_drags>
				</condition>
			</or>
			<hook id="update_option_main"/>
		</event:composite>
		

		<!--システムタブ　更新イベント-->
		<event:composite id="update_tab_sys">
			<!-- 被覆状態のチェック -->
			<or>
				<condition>
					<and>
						<!--押下,無効状態じゃなかったら-->
						<not><predicate_point_button_on button="left"/></not>
						<predicate_point_hover_in/>
						<not><predicate_disable/></not>
					</and>
					<event:composite>
						<or>
							<!--押下状態じゃなかったら被覆のSEを鳴らす-->
							<condition>
							<and>
								<not><predicate_hold/></not>
								<not><predicate_active/></not>
							</and>
								<event:composite>
									<play_sound:eden label="se_m_over"/>
									<change_cursor label="button"/>
								</event:composite>
							</condition>
						</or>
						
						<hover_enable/>
					</event:composite>
				</condition>
				
				<condition><predicate_point_hover_out/>
					<event:composite>
						<hover_disable/>
						<restore_cursor/>
					</event:composite>
				</condition>
			</or>
			
			<!--無効状態でない時-->
			<condition><not><predicate_disable/></not>
				<or>
					<!-- 左クリックのチェック -->
					<condition>
						<and>
							<predicate_point_hold_in button="left"/>
							<not><predicate_active/></not>
						</and>
						<event:composite>
							<hold_enable/>
							<play_sound:eden label="se_mr_sub_ok"/>
						</event:composite>
					</condition>
					<!-- 被覆しておらずボタンが離されたら、メニュー要素の押下状態を無効にする。-->
					<condition><not><predicate_point_hold_out button="left"/></not>
						<condition><predicate_point_button_off button="left"/>
							<hold_disable/>
						</condition>
					</condition>
				</or>
			</condition>
			
			<condition><predicate_hold/>
				<event:composite>
					<!-- 他のタブを非アクティブに変更 -->
					<other component="tab_op"><active_disable/></other>
					<other component="tab_inf"><active_disable/></other>
					<!-- 自分自身をアクティブに変更 -->
					<active_enable/>
					<hook id="hold_in_tab_sys"/>
					<hold_disable/>
				</event:composite>
			</condition>
		</event:composite>
		
